{
  "name": "whispr-server",
  "version": "0.1.0",
  "description": "Decentralised server instance",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --files -r tsconfig-paths/register --exit-child src/index.ts",
    "start": "ts-node --files -r tsconfig-paths/register src/index.ts",
    "lint": "eslint src",
    "prisma:update": "pnpm exec prisma db push && pnpm exec prisma generate",
    "prisma:studio": "pnpm exec prisma studio",
    "prisma:reset": "ts-node -r tsconfig-paths/register ./scripts/prismaReset.ts"
  },
  "keywords": [],
  "author": "Morgan Dilling",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.4",
    "@types/node": "^18.13.0",
    "@types/response-time": "^2.3.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.0.1",
    "eslint": "^8.7.0",
    "prettier": "^2.5.1",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-ws": "^5.0.2",
    "response-time": "^2.3.2",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  }
}
